// callback to promise 

// callback 
function medicine1Get(){
    console.log("medicine 1 received ");
}

setTimeout(medicine1Get,1000)
// to promise - - 

function medicine1Get(){
    return new Promise(function(resolve){
        setTimeout(resolve,1000)
    })
   
}

medicine1Get().then(function(){
    console.log("medicine 1 received ");
})
